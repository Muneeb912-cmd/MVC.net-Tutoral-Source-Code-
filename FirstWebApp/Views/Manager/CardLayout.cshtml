@model IEnumerable<FirstWebApp.Table>

@{
    ViewBag.Title = "CardLayout";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="text-gray-600 body-font">
    <div class="container px-5 py-24 mx-auto">
        <div class="flex flex-wrap -m-4">
            @foreach (var item in Model)
            {



                <div class="p-4 lg:w-1/3">
                    <div class="h-full bg-gray-100 bg-opacity-75 px-8 pt-16 pb-24 rounded-lg overflow-hidden text-center relative">
                        <h2 class="tracking-widest text-xs title-font font-medium text-gray-400 mb-1">Data for</h2>
                        <h1 class="title-font sm:text-2xl text-xl font-medium text-gray-900 mb-3">@Html.DisplayFor(modelItem => item.FirstName) @Html.DisplayFor(modelItem => item.LastName) </h1>
                        <p class="leading-relaxed mb-3">Email : @Html.DisplayFor(modelItem => item.Email)<br>Phone : @Html.DisplayFor(modelItem => item.Phone)<br />Password : @Html.DisplayFor(modelItem => item.Password)<br />BirthDay : @Html.DisplayFor(modelItem => item.BirthDay)</p>
                    </div>
                </div>



            }
        </div>
        </div>
    </section>

